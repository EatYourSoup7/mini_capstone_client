<h1>All Products</h1>
<% @products.each do |product| %>
  <h2><a href="/client/products/<%= product["id"] %>"><%= product["name"] %></a></h2>
  <h3>Description: <%= product["description"] %></h3>
  <h4>Price: <%= product["price"] %></h4>
  <h4><img src="<%= product["image_url"] %>" alt="<%= product["title"] %>" width="300"></h4>
  <br>

<% end %>


